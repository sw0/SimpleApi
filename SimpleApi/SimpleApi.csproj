    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>9511cee9-af14-4872-8c5f-e9f760a71bd1</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
